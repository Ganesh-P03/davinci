class Main{
    function int main() {
        return Main.fib(10);
    }
  
    function int fib(int n) {
      var int a;
      var int b;
      var int c;
      var int i;
  
      let i = 2;
      let a = 0;
      let b = 1;
  
      if(n = 1){
        return 0;
      }
  
      while(i < n){
        let c = a + b;
        let a = b;
        let b = c;
  
        let i = i + 1;
      }
  
      return b;
    }
  
  }